{
  "name": "use-hangul-typewriter",
  "version": "0.5.1",
  "description": "react hook for hangul typewriter effect",
  "main": "./src/index.ts",
  "types": "dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.cjs.json & tsc --project tsconfig.esm.json",
    "build:example": "npm run clean && npm run build && tsc ./example/index.tsx --jsx react-jsx -esModuleInterOp && webpack --config ./example/webpack.config.js",
    "clean": "rm -rf dist",
    "prepublish": "npm run build:example",
    "webpack": "webpack --watch --config ./example/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "hook",
    "hangul",
    "typewriter"
  ],
  "author": "Dongwon Lim",
  "license": "MIT",
  "peerDependencies": {
    "hangul-js": "^0.2.6",
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.16",
    "hangul-js": "^0.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.3.5",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2"
  }
}
